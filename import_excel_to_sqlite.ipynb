    "\n",
    "conn = sqlite3.connect(\"mydb.db\")\n",
    "\n",
    "df.to_sql(\"sales1\", conn, if_exists=\"append\", index=False)\n",
    "\n",
    "conn.close()\n",
    "\n",
    "print(\"Import completed successfully\")\n"
